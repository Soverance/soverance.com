@{
    ViewData["Title"] = "CTF-Elemental";
}

<!-- PAGE NAME START -->
<section class="page-name parallax" style="background-image: url(../images/portfolio/ctf-elemental/page-name.jpg)" data-paroller-factor="0.1" data-paroller-type="background" data-paroller-direction="vertical">
    <div class="container">
        <div class="row">
            <h1 class="page-title">
                CTF-Elemental
            </h1>
            <div class="breadcrumbs">
                <a asp-area="" asp-controller="Home" asp-action="Index">Home</a> /
                <a href="#">Studio</a> /
                <a asp-area="" asp-controller="Home" asp-action="Portfolio">Portfolio</a> /
                <span class="color-1">CTF-Elemental</span>
            </div>
        </div>
    </div>
</section>
<!-- PAGE NAME END -->
<!-- SECTION START -->
<section class="ptb150">
    <div class="container">
        <div class="row">
            <div class="top-bl">
                <div class="game-img-slider bg-arrow">
                    <div class="image-wrapper">
                        <a href="~/images/portfolio/ctf-elemental/ut1.jpg" data-lightbox="game-slides">
                            <img src="~/images/portfolio/ctf-elemental/small-ut1.jpg" alt="" class="img-responsive">
                        </a>
                    </div>
                    <div class="image-wrapper">
                        <a href="~/images/portfolio/ctf-elemental/ut2.jpg" data-lightbox="game-slides">
                            <img src="~/images/portfolio/ctf-elemental/small-ut2.jpg" alt="" class="img-responsive">
                        </a>
                    </div>
                    <div class="image-wrapper">
                        <a href="~/images/portfolio/ctf-elemental/ut3.jpg" data-lightbox="game-slides">
                            <img src="~/images/portfolio/ctf-elemental/small-ut3.jpg" alt="" class="img-responsive">
                        </a>
                    </div>
                    <div class="image-wrapper">
                        <a href="~/images/portfolio/ctf-elemental/ut4.jpg" data-lightbox="game-slides">
                            <img src="~/images/portfolio/ctf-elemental/small-ut4.jpg" alt="" class="img-responsive">
                        </a>
                    </div>
                    <div class="image-wrapper">
                        <a href="~/images/portfolio/ctf-elemental/ut5.jpg" data-lightbox="game-slides">
                            <img src="~/images/portfolio/ctf-elemental/small-ut5.jpg" alt="" class="img-responsive">
                        </a>
                    </div>
                    <div class="image-wrapper">
                        <a href="~/images/portfolio/ctf-elemental/ut6.jpg" data-lightbox="game-slides">
                            <img src="~/images/portfolio/ctf-elemental/small-ut6.jpg" alt="" class="img-responsive">
                        </a>
                    </div>
                    <div class="image-wrapper">
                        <a href="~/images/portfolio/ctf-elemental/ut7.jpg" data-lightbox="game-slides">
                            <img src="~/images/portfolio/ctf-elemental/small-ut7.jpg" alt="" class="img-responsive">
                        </a>
                    </div>
                    <div class="image-wrapper">
                        <a href="~/images/portfolio/ctf-elemental/ut8.jpg" data-lightbox="game-slides">
                            <img src="~/images/portfolio/ctf-elemental/small-ut8.jpg" alt="" class="img-responsive">
                        </a>
                    </div>
                    <div class="image-wrapper">
                        <a href="~/images/portfolio/ctf-elemental/ut9.jpg" data-lightbox="game-slides">
                            <img src="~/images/portfolio/ctf-elemental/small-ut9.jpg" alt="" class="img-responsive">
                        </a>
                    </div>
                    <div class="image-wrapper">
                        <a href="~/images/portfolio/ctf-elemental/ut10.jpg" data-lightbox="game-slides">
                            <img src="~/images/portfolio/ctf-elemental/small-ut10.jpg" alt="" class="img-responsive">
                        </a>
                    </div>
                    <div class="image-wrapper">
                        <a href="~/images/portfolio/ctf-elemental/ut11.jpg" data-lightbox="game-slides">
                            <img src="~/images/portfolio/ctf-elemental/small-ut11.jpg" alt="" class="img-responsive">
                        </a>
                    </div>
                    <div class="image-wrapper">
                        <a href="~/images/portfolio/ctf-elemental/ut12.jpg" data-lightbox="game-slides">
                            <img src="~/images/portfolio/ctf-elemental/small-ut12.jpg" alt="" class="img-responsive">
                        </a>
                    </div>
                </div>
            </div>
        </div>
        <div class="row mt80">
            <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12 mb30">
                <div class="tab-head table uppercase fsize-14 fweight-700">
                    <div class="table-cell valign-middle ws-20 color-1">
                        Unreal Tournament Mod
                    </div>
                    <div class="table-cell valign-middle text-right">
                        Unavailable
                    </div>
                </div>
                <div class="uppercase fsize-32 fweight-700 font-agency color-white lheight-normal">
                    CTF-Elemental
                </div>
                <div class="mt50 lheight-26 fweight-300">
                    <p>
                        CTF-Elemental is a custom capture-the-flag map mod for the latest version of Epic's Unreal Tournament.
                    </p>
                    <p>
                        Originally released in March of 2015, the CTF-Elemental map was based on the assets from Epic's GDC 2012 tech demo, <i>Elemental</i>.
                        The map had one major revision (to version 0.2), but unfortunately did not receive any further updates, and now is no longer playable.
                    </p>
                    <p>
                        One day we hope to make an update to this!
                    </p>
                </div>
            </div>
            <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12 mb30">
                <div class="default-tabs">
                    <div class="image-bl">
                        <video id="elemental" class="vjs-big-play-centered video-js vjs-default-skin vjs-16-9 game-full-img" controls preload="auto" width="570" height="450"
                               data-setup='{ "techOrder": ["youtube"], "sources": { "type": "video/youtube", "src": "https://www.youtube.com/watch?v=VfxeolE-_p8"}}'></video>
                    </div>
                </div>
            </div>
            <div class="widget-wrapper">
                <br />
                <table class="table list-1">
                    <tbody>
                        <tr>
                            <td>
                                <div class="table table-nobackground">
                                    <div class="table-row">
                                        <div class="latest-title table-cell valign-middle text-center pl20">
                                            <div class="title-top font-agency fweight-700 uppercase color-white">
                                                <i class="fa fa-shield fsize-80 fweight-400 color-6"></i>
                                                <p>Visceral Combat</p>
                                            </div>
                                            <p>Experience a new way to enjoy the hectic first-person sci-fi combat of Unreal Tournament.</p>
                                        </div>
                                    </div>
                                </div>
                            </td>
                            <td>
                                <div class="table table-nobackground">
                                    <div class="table-row">
                                        <div class="latest-title table-cell valign-middle text-center pl20">
                                            <div class="title-top font-agency fweight-700 uppercase color-white">
                                                <i class="fa fa-fire fsize-80 fweight-400 color-6"></i>
                                                <p>Elemental Theme</p>
                                            </div>
                                            <p>Fight your way through a gorgeous environment of fire and ice, inspired by Epic's GDC 2012 demo, <i>Elemental</i>.</p>
                                        </div>
                                    </div>
                                </div>
                            </td>
                            <td>
                                <div class="table table-nobackground">
                                    <div class="table-row">
                                        <div class="latest-title table-cell valign-middle text-center pl20">
                                            <div class="title-top font-agency fweight-700 uppercase color-white">
                                                <i class="fa fa-eye fsize-80 fweight-400 color-6"></i>
                                                <p>Twist on CTF</p>
                                            </div>
                                            <p>In CTF-Elemental, you play to dominate the arena held by two powerful Elementals, caught in eternal conflict. Summon the Elementals to protect the flag and grant bonuses to your team.</p>
                                        </div>
                                    </div>
                                </div>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12 st-tabs tabs mt70">
                <div class="tabs-nav">
                    <ul class="list-inline nav nav-tabs">
                        <li class="active col-lg-4 col-md-4 col-sm-12 col-xs-12">
                            <a href="#stream-2" data-toggle="tab">
                                <div class="tab-title">
                                    CTF-Elemental YouTube Playlist
                                </div>
                            </a>
                        </li>
                    </ul>
                </div>
                <div class="tab-content stream-bl relative">
                    <div class="tab-pane fade active in competition text-left clearfix" id="stream-2">
                        <div class="left-col col-lg-6 col-md-6 col-sm-12 col-xs-12">
                            <div class="video-bl equal-height">
                                <video id="LatestVideo" class="vjs-big-play-centered video-js vjs-default-skin vjs-16-9 game-full-img" controls preload="auto"
                                       data-setup='{ "techOrder": ["youtube"], "sources": { "type": "video/youtube", "src": "@ViewBag.ShowcaseVideo.VideoWatchLink"}}'></video>
                                <div class="video-bottom">
                                    <div class="video-title">
                                        @ViewBag.ShowcaseVideo.Title
                                    </div>
                                    <div class="mt15">
                                        <div class="inline-block valign-middle color-white ml15">
                                            @ViewBag.ShowcaseVideo.ViewCount views
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="right-col col-lg-6 col-md-6 col-sm-12 col-xs-12 equal-height">
                            @foreach (var video in ViewBag.ElementalPlaylist)
                            {
                                <div class="item-g">
                                    <div class="img-com inline-block valign-middle equal-height">
                                        <img src="@video.ImageUrl" alt="" height="80" width="80">
                                    </div>
                                    <div class="info-bl inline-block valign-middle ml25">
                                        <a href="@video.VideoWatchLink" class="fsize-24 fweight-700 font-agency uppercase color-white" data-trim="35">@video.Title</a>
                                        <div>@video.ViewCount views</div>
                                    </div>
                                </div>
                            }
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- SECTION END -->