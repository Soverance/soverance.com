
<div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
    @if (ViewData["User"] != null)
    {
        <pre>USER:  @ViewData["User"]</pre>
    }
    @if (ViewData["Password"] != null)
    {
        <pre>PASS:  @ViewData["Password"]</pre>
    }
    @if (ViewData["Link"] != null)
    {
        <pre>LINK:  @ViewData["Link"]</pre>
    }
    @if (ViewData["Message"] != null)
    {
        <pre>INFO:  @ViewData["Message"]</pre>
    }
</div>


